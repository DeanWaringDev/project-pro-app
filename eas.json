{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "latest",
        "resourceClass": "large"
      },
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": ""
      },
      "hooks": {
        "prebuild": [
          {
            "file": "hooks/prebuild.js"
          }
        ]
      }
    }
  }
}